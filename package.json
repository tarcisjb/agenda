{
  "name": "server",
  "version": "1.0.0",
  "description": "Cadastro de agendas de psicologia",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf js/app && mkdir js/app",
    "build-babel": "babel js/app-es6 -d js/app --source-maps",
    "watch": "babel js/app-es6 -d js/app --source-maps --watch",
    "build": "npm run clean && npm run build-babel",
    "compile-ts": "tsc",
    "start-ts": "tsc -w",
    "start-all": "concurrently \"npm run start-ts\" \"npm run start\"",
    "start": "node server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-systemjs": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/jquery": "^3.3.38",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "systemjs": "^6.3.1",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": "12.16.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
